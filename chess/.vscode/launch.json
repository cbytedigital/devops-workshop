  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug current test file",
      "program": "${workspaceRoot}/node_modules/tape/bin/tape",
      "args": [
        "-r", "./test/tools/babel-browserless-testing.js", "${file}"
      ],
      "env": {
        "NODE_ENV": "test"
      }
    }
  ]
}
